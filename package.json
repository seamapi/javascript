{
  "name": "seam",
  "version": "1.105.0",
  "description": "JavaScript SDK for the Seam API written in TypeScript.",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "module": "index.js",
  "sideEffects": false,
  "keywords": [
    "node"
  ],
  "homepage": "https://github.com/seamapi/javascript",
  "bugs": "https://github.com/seamapi/javascript/issues",
  "repository": "seamapi/javascript",
  "license": "MIT",
  "author": {
    "name": "Seam Labs, Inc.",
    "email": "devops@getseam.com"
  },
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "npm run build:entrypoints",
    "prebuild": "tsx src/index.ts",
    "postbuild": "node ./index.js",
    "build:entrypoints": "npm run build:ts",
    "postbuild:entrypoints": "tsup",
    "build:ts": "tsc --project tsconfig.build.json",
    "prebuild:ts": "del 'index.*'",
    "postbuild:ts": "tsc-alias --project tsconfig.build.json",
    "typecheck": "tsc",
    "test": "true",
    "pretest": "tsx src/index.ts",
    "lint": "eslint .",
    "postlint": "prettier --check --ignore-path .gitignore .",
    "postversion": "git push --follow-tags",
    "generate": "markdown-toc -i README.md --bullets '-'",
    "pregenerate": "tsx generate-readme.ts",
    "postgenerate": "prettier --write README.md",
    "example": "tsx examples",
    "example:inspect": "tsx --inspect examples",
    "format": "prettier --write --ignore-path .gitignore .",
    "preformat": "eslint --fix ."
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.1.0"
  },
  "dependencies": {
    "@seamapi/http": "1.81.0",
    "@seamapi/types": "1.603.0",
    "@seamapi/webhook": "1.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/core": "^1.11.29",
    "@types/node": "^22.15.21",
    "del-cli": "^6.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jiti": "^2.4.2",
    "landlubber": "^2.0.0",
    "markdown-toc": "^1.2.0",
    "neostandard": "^0.12.2",
    "prettier": "^3.0.0",
    "tsc-alias": "^1.8.2",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.8.3"
  }
}
